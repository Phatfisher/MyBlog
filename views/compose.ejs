<%- include("partials/header")-%>

<h1>Compose</h1>
<form id="" action="/compose" method="POST">
    <div class="form-group">
        <h5><b>Title</b></h5>
        <input class="form-control" type="text" name="newBlogTitle" placeholder="Title" autocomplete="off">
    </div>
    <div class="form-group">
        <h5><b>Post</b></h5>
        <textarea class="form-control" name="newBlogContent" rows="3"></textarea>
    </div>
    <br>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer")-%>